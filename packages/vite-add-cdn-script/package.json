{
  "name": "vite-add-cdn-script",
  "version": "1.0.3",
  "keywords": [
    "vite",
    "cdn",
    "cdn-script",
    "auto-add-cdn",
    "vite-add-cdn-script"
  ],
  "description": "A plugin for vite to add cdn script to index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baizeteam/baize-package.git"
  },
  "homepage": "https://github.com/baizeteam/baize-package/tree/release/package/vite-add-cdn-script#readme",
  "author": "baizeteam",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "build:page": "tsc && cross-env BUILD_MODE=page vite build",
    "build": "tsc && cross-env BUILD_MODE=lib vite build",
    "test": ""
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.12.11",
    "@types/node-fetch": "2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/semver": "^7.5.8",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "rollup-plugin-external-globals": "^0.10.0",
    "rollup-plugin-node-externals": "^7.1.2",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite-plugin-dts": "^3.9.1",
    "vite": "^5.2.11"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "cdn-script-core": "workspace:*"
  }
}
